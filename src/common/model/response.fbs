include "keyvalue.fbs";

namespace spt.configdb.model;

union ValueVariant{ Value, Children, Success }

table KeyValueResult
{
  key:string;
  value:ValueVariant;
}

table KeyValueResults
{
  value:[KeyValueResult];
}

union ResultVariant { KeyValueResults, Success }

table Response
{
  value:ResultVariant;
}

root_type Response;

